{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "suggest.triggerAfterInsertEnter": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "css",
    "sass",
    "vue",
    "markdown",
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  //diagnostic
  "diagnostic.signOffset": 9999999,
  "diagnostic.errorSign": "●",
  "diagnostic.warningSign": "●",
  "diagnostic.infoSign": "●",
  "diagnostic.displayByAle": false,
  "diagnostic.refreshOnInsertMode": true,
  // coc-explorer
  "explorer.icon.expanded": "▾",
  "explorer.icon.collapsed": "▸",
  "explorer.icon.enableNerdfont": true,
  "explorer.quitOnOpen": false,
  "explorer.openAction.strategy": "sourceWindow",
  "explorer.file.hiddenRules": {
    "extensions": ["o", "a", "obj", "pyc"],
    "filenames": [],
    "patternMatches": [".git"]
  },
  "explorer.file.column.git.showIgnored": true,
  //git
  "git.enableGutters": true,
  "git.branchCharacter": "\uf418",
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": true,
  "git.virtualTextPrefix": " ❯❯❯ ",
  "git.addedSign.text": "▋",
  "git.changedSign.text": "▋",
  "git.removedSign.text": "▋",
  "git.topRemovedSign.text": "▔",
  "git.changeRemovedSign.text": "▋",
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  }
}
