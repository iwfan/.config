{
  // coc settings
  "coc.preferences.hoverTarget": "preview",
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.snippets.enable": true,
  "coc.preferences.snippetStatusText": "Ⓢ ",
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "css",
    "sass",
    "vue",
    "markdown",
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx"
  ],

  // suggest
  "suggest.detailMaxLength": 60,
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.autoTrigger": "always",
  "suggest.timeout": 5000,
  "suggest.enablePreview": true,
  "suggest.floatEnable": true,
  "suggest.detailField": "preview",
  "suggest.echodocSupport": true,
  "suggest.completionItemKindLabels": {
    "class": "\uf0e8",
    "color": "\ue22b",
    "constant": "\uf8fe",
    "default": "\uf29c",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "event": "\ufacd",
    "field": "\uf93d",
    "file": "\uf723",
    "folder": "\uf115",
    "function": "\u0192",
    "interface": "\uf417",
    "keyword": "\uf1de",
    "method": "\uf6a6",
    "module": "\uf40d",
    "operator": "\uf915",
    "property": "\ue624",
    "reference": "\ufa46",
    "snippet": "\ue60b",
    "struct": "\ufb44",
    "text": "\ue612",
    "typeParameter": "\uf728",
    "unit": "\uf475",
    "value": "\uf89f",
    "variable": "\ue71b"
  },
  "suggest.maxCompleteItemCount": 20,

  // coc-explorer
  "explorer.icon.expanded": "▾",
  "explorer.icon.collapsed": "▸",
  "explorer.icon.enableNerdfont": true,
  "explorer.quitOnOpen": false,
  "explorer.openAction.strategy": "sourceWindow",
  "explorer.file.hiddenRules": {
    "extensions": ["o", "a", "obj", "pyc"],
    "filenames": [".git"],
    "patternMatches": []
  },
  "explorer.file.column.git.showIgnored": true,

  "bookmark.sign": "\uf461",
  "bookmark.signHl": "Tag",

  // list
  "list.previewHighlightGroup": "Statement",
  "list.source.files.args": ["--hidden", "--files"],

  // coc source
  "coc.source.around.firstMatch": false,
  "coc.source.buffer.firstMatch": false,
  "coc.source.file.ignoreHidden": false,

  // imselect
  "imselect.defaultInput": "com.apple.keylayout.ABC",
  "imselect.enableStatusItem": true,
  "imselect.enableFloating": false,

  // pairs
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],

  //diagnostic
  "diagnostic.enable": true,
  "diagnostic.signOffset": 9999999,
  "diagnostic.highlightOffset": 9999999,
  "diagnostic.errorSign": "\uf05c",
  "diagnostic.warningSign": "\uf071",
  "diagnostic.infoSign": "\uf06a",
  "diagnostic.hintSign": "\uf864",
  "diagnostic.displayByAle": false,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.refreshAfterSave": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": " ❯❯❯ ",
  "diagnostic.virtualText": true,
  "diagnostic.separateRelatedInformationAsDiagnostics": true,

  //git
  "git.enableGutters": true,
  "git.branchCharacter": "\uf418",
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": true,
  "git.virtualTextPrefix": " ❯❯❯ ",
  "git.addedSign.text": "▋",
  "git.changedSign.text": "▋",
  "git.removedSign.text": "▋",
  "git.topRemovedSign.text": "▔",
  "git.changeRemovedSign.text": "▋",
  "git.addedSign.hlGroup": "GitAddHi",
  "git.changedSign.hlGroup": "GitModifyHi",
  "git.removedSign.hlGroup": "GitDeleteHi",
  "git.topRemovedSign.hlGroup": "GitDeleteHi",
  "git.changeRemovedSign.hlGroup": "GitDeleteHi",

  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,

  // prettier
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "prettier.statusItemText": "Ⓟ",

  "cSpell.fixSpellingWithRenameProvider": true,
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "gitcommit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "python",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml",
    "vim"
  ],
  "cSpell.userWords": [
    "Neovim",
    "Nerdfont",
    "Signining",
    "Unimported",
    "bignumber",
    "botright",
    "bowser",
    "ccls",
    "cnpm",
    "cocactions",
    "configrc",
    "eelixir",
    "emment",
    "eruby",
    "gitignore",
    "iamcco",
    "imselect",
    "keylayout",
    "langserver",
    "languagetool",
    "markdownlint",
    "maxsize",
    "nmap",
    "nvim",
    "objc",
    "objcpp",
    "operatorfunc",
    "sfile",
    "shellcheck",
    "shrinked",
    "smartf",
    "struct",
    "stylelint",
    "ultisnips",
    "vimbackupfile",
    "viml",
    "vimlsp",
    "vsplit"
  ],

  "languageserver": {
    "sh": {
      "command": "bash-language-server",
      "args": ["start", "--stdio"],
      "filetypes": ["sh"],
      "cwd": "./",
      "initializationOptions": {},
      "ignoredRootPaths": ["~"],
      "settings": {}
    },
  }
}
