{
  // coc settings
  "coc.preferences.hoverTarget": "preview",
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.snippets.enable": true,
  "coc.preferences.snippetStatusText": "‚ìà ",
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "css",
    "sass",
    "vue",
    "markdown",
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "jsonc",
    "javascriptreact",
    "typescript.tsx",
    "vim"
  ],

  // suggest
  "suggest.detailMaxLength": 60,
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.autoTrigger": "always",
  "suggest.timeout": 5000,
  "suggest.enablePreview": true,
  "suggest.floatEnable": true,
  "suggest.detailField": "preview",
  "suggest.echodocSupport": true,
  "suggest.completionItemKindLabels": {
    "class": "\uf0e8",
    "color": "\ue22b",
    "constant": "\uf8fe",
    "default": "\uf29c",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "event": "\ufacd",
    "field": "\uf93d",
    "file": "\uf723",
    "folder": "\uf115",
    "function": "\u0192",
    "interface": "\uf417",
    "keyword": "\uf1de",
    "method": "\uf6a6",
    "module": "\uf40d",
    "operator": "\uf915",
    "property": "\ue624",
    "reference": "\ufa46",
    "snippet": "\ue60b",
    "struct": "\ufb44",
    "text": "\ue612",
    "typeParameter": "\uf728",
    "unit": "\uf475",
    "value": "\uf89f",
    "variable": "\ue71b"
  },
  "suggest.maxCompleteItemCount": 20,

  // coc-explorer
  "explorer.icon.expanded": "‚ñæ",
  "explorer.icon.collapsed": "‚ñ∏",
  "explorer.icon.enableNerdfont": true,
  "explorer.quitOnOpen": true,
  "explorer.openAction.strategy": "sourceWindow",
  "explorer.file.hiddenRules": {
    "extensions": ["o", "a", "obj", "pyc"],
    "filenames": [".git"],
    "patternMatches": []
  },
  "explorer.git.showIgnored": true,
  "explorer.file.git.showUntrackedFiles": true,
  "explorer.git.icon.status.mixed": "*",
  "explorer.git.icon.status.added": "‚úö",
  "explorer.git.icon.status.copied": "@",
  "explorer.git.icon.status.deleted": "‚úñ",
  "explorer.git.icon.status.renamed": "‚ûú",
  "explorer.git.icon.status.ignored": "‚òí",
  "explorer.git.icon.status.modified": "‚úπ",
  "explorer.git.icon.status.unmerged": "‚ïê",
  "explorer.git.icon.status.untracked": "?",
  "explorer.git.icon.status.unmodified": " ",

  "bookmark.sign": "\uf461",
  "bookmark.signHl": "Tag",

  "signature.target": "float",

  // list
  "list.previewHighlightGroup": "Statement",
  "list.source.files.args": ["--hidden", "--files"],

  // coc source
  "coc.source.around.firstMatch": false,
  "coc.source.buffer.firstMatch": false,
  "coc.source.file.ignoreHidden": false,

  "cSpell.enabled": false,

  // imselect
  "imselect.defaultInput": "com.apple.keylayout.ABC",
  "imselect.enableStatusItem": true,
  "imselect.enableFloating": false,

  //diagnostic
  "diagnostic.enable": true,
  "diagnostic.highlightOffset": 9999999,
  "diagnostic.errorSign": "\uf05c",
  "diagnostic.warningSign": "\uf071",
  "diagnostic.infoSign": "\uf06a",
  "diagnostic.hintSign": "\uf864",
  "diagnostic.displayByAle": false,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": " ‚ùØ‚ùØ‚ùØ ",
  "diagnostic.virtualText": true,
  "diagnostic.separateRelatedInformationAsDiagnostics": true,

  //git
  "git.enableGutters": true,
  "git.branchCharacter": "\uf418",
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": true,
  "git.virtualTextPrefix": " üëâ ",
  "git.addedSign.text": "‚ñã",
  "git.changedSign.text": "‚ñã",
  "git.removedSign.text": "‚ñã",
  "git.topRemovedSign.text": "‚ñî",
  "git.changeRemovedSign.text": "‚ñã",

  "codeLens.enable": true,

  "html.format.enable": true,
  "javascript.referencesCodeLens.enable": true,
  "javascript.showUnused": true,
  "javascript.suggest.names": true,
  "javascript.suggestionActions.enabled": true,
  // emmet
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },

  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,

  // prettier
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "prettier.statusItemText": "ü•≥",
  "prettier.printWidth": 100,
  "prettier.disableLanguages": [],
  "prettier.formatterPriority": 1,
  "prettier.useTabs": false,
  "prettier.trailingComma": "all",
  "prettier.singleQuote": false,

  "cSpell.fixSpellingWithRenameProvider": true,
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "gitcommit",
    "go",
    "handlebars",
    "haskell",
