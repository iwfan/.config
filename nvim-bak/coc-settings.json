  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start", "--stdio"],
      "filetypes": ["sh"],
      "cwd": "./",
      "initializationOptions": {},
      "ignoredRootPaths": ["~"],
      "settings": {}
    }
  }
}
